    "ethers-gcp-kms-signer": "^1.1.6",
    "fastify": "^4.28.1",
    "fastify-plugin": "^4.5.0",
    "http-status-codes": "^2.2.0",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "mnemonist": "^0.39.8",
    "node-cron": "^3.0.2",
    "pg": "^8.11.3",
    "prisma": "^5.14.0",
    "prom-client": "^15.1.3",
    "superjson": "^2.2.1",
