{
  "name": "engine",
  "version": "0.1.0",
  "license": "Apache-2.0",
  "description": "thirdweb Engine",
  "author": "thirdweb engineering <eng@thirdweb.com>",
  "type": "module",
  "engines": {
    "node": ">=22.1.0",
    "pnpm": ">=10.2.1"
  },
  "scripts": {
    "dev": "pnpm dev:infra && pnpm dev:db && pnpm dev:run",
    "dev:infra": "docker compose -f ./docker-compose.yml up -d",
    "dev:db": "pnpm prisma:setup:dev",
    "dev:run": "tsx watch src/index.ts",
    "build": "rm -rf dist && tsc -p ./tsconfig.json",
    "build:docker": "docker build . -f Dockerfile -t prod",
    "generate:sdk": "tsx ./src/scripts/generate-sdk && cd ./sdk && pnpm build",
    "prisma:setup:dev": "tsx ./src/scripts/setup-db.ts",
    "prisma:setup:prod": "tsx ./dist/scripts/setup-db.js",
    "start": "pnpm prisma:setup:prod && pnpm start:migrations && pnpm start:run",
    "start:migrations": "tsx ./dist/scripts/apply-migrations.js",
    "start:run": "node ./dist/index.js",
    "start:docker": "docker compose --profile engine --env-file ./.env up --remove-orphans",
    "start:docker-force-build": "docker compose --profile engine --env-file ./.env up --remove-orphans --build",
    "test:unit": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "pnpm biome lint",
    "copy-files": "cp -r ./src/prisma ./dist/"
  },
  "dependencies": {
    "@aws-sdk/client-kms": "^3.679.0",
    "@bull-board/api": "^6.7.7",
    "@bull-board/fastify": "^6.7.7",
    "@circle-fin/developer-controlled-wallets": "^7.0.0",
    "@cloud-cryptographic-wallet/cloud-kms-signer": "^0.1.2",
    "@cloud-cryptographic-wallet/signer": "^0.0.5",
    "@ethersproject/json-wallets": "^5.7.0",
    "@fastify/swagger": "^9.4.2",
    "@fastify/type-provider-typebox": "^5.1.0",
    "@fastify/websocket": "^11.0.0",
    "@google-cloud/kms": "^4.4.0",
    "@prisma/client": "6.3.1",
    "@sinclair/typebox": "^0.31.28",
    "@t3-oss/env-core": "^0.6.0",
    "@thirdweb-dev/auth": "^4.1.87",
    "@thirdweb-dev/chains": "^0.1.77",
    "@thirdweb-dev/sdk": "^4.0.89",
    "@thirdweb-dev/service-utils": "^0.4.28",
    "@thirdweb-dev/wallets": "^2.5.39",
    "@types/base-64": "^1.0.2",
    "abitype": "^1.0.8",
    "aws-kms-signer": "^0.5.3",
    "base-64": "^1.0.0",
    "bullmq": "^5.11.0",
    "cron-parser": "^4.9.0",
    "crypto": "^1.0.1",
    "crypto-js": "^4.2.0",
    "dd-trace": "^5.23.0",
    "dotenv": "^16.0.3",
    "ethers": "5",
    "ethers-aws-kms-signer": "^1.3.2",
    "ethers-gcp-kms-signer": "^1.1.6",
    "fastify": "^5.2.0",
    "fastify-plugin": "^5.0.1",
    "http-status-codes": "^2.2.0",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "lru-cache": "^11.0.2",
    "mnemonist": "^0.39.8",
    "node-cron": "^3.0.2",
    "ox": "^0.6.9",
    "pg": "^8.11.3",
    "prisma": "^6.3.1",
    "prom-client": "^15.1.3",
    "superjson": "^2.2.1",
    "thirdweb": "^5.83.0",
    "undici": "^6.20.1",
    "uuid": "^9.0.1",
    "viem": "^2.21.54",
    "winston": "^3.14.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.2",
    "@types/cli-progress": "^3.11.3",
    "@types/crypto-js": "^4.2.2",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^22.1.0",
    "@types/node-cron": "^3.0.8",
    "@types/pg": "^8.6.6",
    "@types/uuid": "^9.0.1",
    "@types/ws": "^8.5.5",
    "@vitest/coverage-v8": "^2.0.3",
    "openapi-typescript-codegen": "^0.25.0",
    "prool": "^0.0.16",
    "tsx": "^4.7.1",
    "typescript": "^5.1.3",
    "vitest": "^2.0.3"
  },
  "prisma": {
    "schema": "./src/prisma/schema.prisma"
  },
  "pnpm": {
    "overrides": {
      "@thirdweb-dev/auth>axios": ">=1.7.8",
      "@thirdweb-dev/auth>web3-utils": ">=4.2.1",
      "@grpc/grpc-js": ">=1.8.22",
      "body-parser": ">=1.20.3",
      "cookie": ">=0.7.0",
      "elliptic": ">=6.6.0",
      "micromatch": ">=4.0.8",
      "secp256k1": ">=4.0.4",
      "ws": ">=8.17.1",
      "cross-spawn": ">=7.0.6"
    },
    "ignoredBuiltDependencies": [
      "@prisma/client",
      "@prisma/engines",
      "prisma"
    ],
    "onlyBuiltDependencies": [
      "@prisma/client",
      "@prisma/engines",
      "prisma"
    ]
  }
}
